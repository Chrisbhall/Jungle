<section class="admin-products-new">
  <header class="page-header">
    <h1>Login</h1>
  </header>  
  <div class="well">
    <%= link_to 'All Accounts', [:account, :accounts], class: 'btn btn-info' %>
  </div>


  <%= form_for :account do |form| %>

      <div class="form-group">
          <%= form.label :email %> 
          <%= form.text_field :email, class: 'form-control' %>
        </div>

        <div class="form-group">
          <%= form.label :password %>
          <%= form.password_field :created_at, class: 'form-control' %>
        </div>

        <%= form.submit 'Login', class: 'btn btn-primary' %>
        <% end %>
    </div>
  </div>
</section>
